{
  "compilerOptions": {
    "target": "ES2022",
    "lib": [
      "ES2022",
      "DOM"
    ],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "jsx": "react-jsx",
    "resolveJsonModule": true,
    "noEmit": true,
    "strict": true,
    "allowJs": false,
    "baseUrl": ".",
    "paths": {},
    "skipLibCheck": true,
    "incremental": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "plugins": []
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}
